# Test Database Configuration
# Copy this to .env.test and modify values for your test environment

# Test PostgreSQL Database URL (separate from production database)
# Format: postgresql://username:password@host:port/database_name
TEST_DATABASE_URL=postgresql://price_tracker_user:pass@123@localhost:5432/price_tracker_test

# Or if using the same database with a prefix:
# TEST_DATABASE_URL=postgresql://price_tracker_user:pass@123@localhost:5432/price_tracker

# Flask Test Configuration
FLASK_ENV=testing
TESTING=1

# CORS Configuration for testing
CORS_ORIGIN=*

# SMTP Configuration (for notification testing - use test credentials)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
EMAIL_ID=test@example.com
EMAIL_PASS=test_app_password

# Optional: Logging level for tests
LOG_LEVEL=DEBUG

